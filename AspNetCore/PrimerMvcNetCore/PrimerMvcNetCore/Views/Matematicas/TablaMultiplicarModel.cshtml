@model List<FilaTablaMultiplicar>

<h2 style="color: mediumvioletred;">
    Tabla Multiplicar Model
</h2>

<form method="post">
    <label>Tabla Multiplicar</label>
    <input type="text" name="numero" class="form-control" />
    <button class="btn btn-outline-danger">Mostrar tabla</button>
</form>

@if (Model != null)
{
    <h1>Tabla del @ViewData["NUMERO"]</h1>
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Operación</th>
                <th>Resultado</th>
            </tr>
        </thead>
        <tbody>
            @foreach (FilaTablaMultiplicar fila in Model){
                <tr>
                    <td>@fila.Operacion</td>
                    <td>@fila.Resultado</td>
                </tr>
            }
        </tbody>
    </table>
}