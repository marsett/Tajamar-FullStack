<h1>Upload Files Net Core</h1>

<form method="post" enctype="multipart/form-data">
    <label>Selecciona un fichero</label>
    <input type="file" name="fichero" class="form-control"/>
    <button class="btn btn-dark">
        Subir fichero
    </button>
</form>

<h2 style="color: red;">@ViewData["MENSAJE"]</h2>
<h2 style="color: green;">@ViewData["PATH"]</h2>

<img src='@ViewData["PATH"]' style="width: 70%; height: 70%;"/>