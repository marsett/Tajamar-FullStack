@model Enfermo

<h1>Detalles Enfermo</h1>

<p>
    <a asp-controller="Enfermos"
       asp-action="Index"
       class="btn btn-warning">
        Volver
    </a>
</p>

<ul class="list-group">
    <li class="list-group-item">
        @Model.Inscripcion
    </li>
    <li class="list-group-item">
        @Model.Apellido
    </li>
    <li class="list-group-item">
        @Model.Direccion
    </li>
    <li class="list-group-item">
        @Model.Fecha_Nac.ToLongDateString()
    </li>
    <li class="list-group-item">
        @Model.Sexo
    </li>
    <li class="list-group-item">
        @Model.Nss
    </li>
</ul>